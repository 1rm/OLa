				bshell($bid, 'net group "domain admins" /domain');
			}
			item "查找企业管理员" {
				blog($1, "\c9=========== 查找企业管理员 ==========");
				$bid = $1['@'];
				bshell($bid, 'net group "enterprise admins" /domain');
			}
			item "查看登录本机的域管" {
				blog($1, "\c9=========== 查看登录本机的域管 ==========");
