				($computer, $user, $pid, $barch) = values($2, @('computer', 'user', 'pid', 'barch'));
				if ("x86" eq $barch) {
					return "[ $+ $computer $+ ] $user $+ / $+ $pid $+ \c4[ $av_list $+ ]";
				}
				else if ("x64" eq $barch) {
					return "[ $+ $computer $+ ] $user $+ / $+ $pid \cE(x64)\o $+ \c4[ $av_list $+ ]";
				}
				else {
					return " $+ \c4[ $av_list $+ ]";
				}
				
			}

		}
		}


