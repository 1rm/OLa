		}

		menu "查询域内用户" {
			item "查询指定域用户信息" {
				blog($1, "\c9=========== 查询指定域用户信息 ==========");
				prompt_text("输入域用户名", "", lambda({bshell(@bid, "net user $1 /domain");}, @bid => $bid));
			}
			item "查询当前域用户 SID" {
				blog($1, "\c9=========== 查询域用户 SID ==========");
				$bid = $1['@'];
				bshell($bid, "whoami /user");
			}
			item "查看所有域用户" {
				blog($1, "\c9=========== 查看所有域用户 ==========");
				bpowershell_import($1, script_resource("/scripts/discovery-tools/Powerview.ps1"));
				bpowerpick($1, "Get-NetUser | select name");
			}
			item "查看域内所有用户组列表" {
				blog($1, "\c9=========== 查看域内所有用户组列表 ==========");
