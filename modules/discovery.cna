				blog($1, "将域内用户详细信息导出到当前目录下的 domain_user.txt 文件中!");
			}
			item "将域用户详细信息导出到文件" {
				blog($1, "\c9=========== 将域用户详细信息导出到文件 ==========");
				bpowershell_import($1, script_resource("/scripts/discovery-tools/Powerview.ps1"));
